# Production Environment Variables Template
# Copy this to .env on the VPS and fill in actual values

# Database Configuration
DATABASE_URL="mysql://pms_user:YOUR_MYSQL_PASSWORD@localhost:3306/ckcm_payroll"

# NextAuth Configuration
NEXTAUTH_URL="http://YOUR_SERVER_IP_OR_DOMAIN"
NEXTAUTH_SECRET="YOUR_NEXTAUTH_SECRET_HERE"

# Google OAuth (Optional - leave empty if not using)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""

# Node Environment
NODE_ENV="production"
